	author = {Rouquette, Maïeul},
	year = {2012},
	note = {Pages: 268},
	file = {Full Text PDF:/home/mathilde/snap/zotero-snap/common/Zotero/storage/V2BHELCW/Rouquette - 2012 - (Xe)LaTeX appliqué aux sciences humaines.pdf:application/pdf},
