@article{vitali-rosati_quest-ce_2020,
	title = {Qu’est-ce que l’écriture numérique ?},
	volume = {HS-33},
	url = {https://journals.openedition.org/corela/11759},
	doi = {https://doi.org/10.4000/corela.11759},
	abstract = {Il est évident que le statut du texte change profondément à cause de l’impact des technologies numériques. D’une part, dans les environnements numériques le texte est partout : les images, les vidéos, les clics, les objets et même les actions sont en réalité des séries de caractères. D’autre part, le texte acquiert une valeur opérationnelle. Comme le montrent Souchier et Jeanneret avec la notion d’“architexte”, le texte fait des choses, il produit d’autres textes ou des actions. La thèse de cet article est que ces changements en impliquent un autre: le texte devient un matériel architectural, dans le sens propre du mot – à savoir, et en dehors des métaphores, il est l’élément qui structure matériellement l’espace dans lequel nous vivons. Cette thèse est à la base de la théorie de l’éditorialisation, notion qui peut être définie comme “l’ensemble des dynamiques qui produisent l’espace numérique.”},
	language = {fr},
	journal = {CORELA},
	author = {Vitali-Rosati, Marcello},
	month = oct,
	year = {2020},
	keywords = {Éditorialisation, Production des contenus, Auteurs et auctorialité, Édition, Édition numérique, Plateformes d'édition},
}

