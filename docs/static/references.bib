	journal = {Digital Studies / Le champ numérique},
	author = {Casenave, Joana},
	month = sep,
	year = {2019},
	note = {Number: 1
Publisher: Open Library of Humanities},
	file = {Full Text PDF:/home/mathilde/snap/zotero-snap/common/Zotero/storage/ZAZS3AQW/Casenave - 2019 - Le positionnement éditorial dans l’édition critiqu.pdf:application/pdf},
}

